{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "put"
      ],
      "route": "private/personas/{id}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "item",
      "databaseName": "organizacion",
      "collectionName": "personas",
      "connectionStringSetting": "CosmosDBConnection",
      "Id":"{id}",
      "PartitionKey":"{id}"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "updatePersona",
      "databaseName": "organizacion",
      "collectionName": "personas",
      "createIfNotExists": false,
      "connectionStringSetting": "CosmosDBConnection"
    }
  ],
  "scriptFile": "../dist/patchPersona/index.js"
}
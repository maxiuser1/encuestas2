{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ],
      "route":"private/campanas"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "campana",
      "databaseName": "organizacion",
      "collectionName": "campanas",
      "createIfNotExists": true,
      "connectionStringSetting": "CosmosDBConnection"
    }
  ],
  "scriptFile": "../dist/postCampana/index.js"
}
{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route":"public/persona"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "item",
      "databaseName": "organizacion",
      "collectionName": "personas",
      "connectionStringSetting": "CosmosDBConnection",
      "partitionKey": "compania"
    }
  ],
  "scriptFile": "../dist/postgerencias/index.js"
}

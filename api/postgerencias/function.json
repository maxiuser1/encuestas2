{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ],
      "route":"private/gerencias"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "gerencia",
      "databaseName": "organizacion",
      "collectionName": "gerencias",
      "createIfNotExists": true,
      "connectionStringSetting": "CosmosDBConnection",
    }
  ],
  "scriptFile": "../dist/postgerencias/index.js"
}

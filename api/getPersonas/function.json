{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route":"private/personas"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "items",
      "databaseName": "organizacion",
      "collectionName": "personas",
      "connectionStringSetting": "CosmosDBConnection",
      "sqlQuery":"SELECT  c.id, c.name, c.email from c order by c.name",
      "PartitionKey":"bciseguros"
    }
  ],
  "scriptFile": "../dist/getPersonas/index.js"
}